sudo docker run -d --name=postgres --rm --env=POSTGRES_PASSWORD=foo \--volume=$(pwd)/media.sql:/docker-entrypoint-initdb.d/media.sql \--volume=$(pwd)/post.sql:/docker-entrypoint-initdb.d/post.sql \--volume=$(pwd)/user.sql:/docker-entrypoint-initdb.d/user.sql \--volume=$(pwd)/charity.sql:/docker-entrypoint-initdb.d/charity.sql \--volume=$(pwd):/repo \postgres:latest -c log_statement=all